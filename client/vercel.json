{
    "version": 2,
    "name": "rupee-master-client",
    "builds": [
      {
        "src": "vite.config.ts",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  